{
  "name": "vue-pdf-cdn",
  "version": "2.0.6",
  "private": false,
  "description": "Lightweight PDF viewer for Vue.js using CDN",
  "author": "leo-buneev <buneev.leonid@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js --open",
    "build": "vue-cli-service build --name vue-pdf-cdn --entry ./src/index.js --target lib",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "main": "dist/vue-pdf-cdn.umd.js",
  "module": "dist/vue-pdf-cdn.common.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@vue/cli-plugin-babel": "~5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.1",
    "@vue/cli-service": "~5.0.0-rc.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "file-loader": "^6.2.0",
    "prettier": "^2.5.1",
    "vue": "^3.0.0"
  },
  "keywords": [
    "vue",
    "component",
    "pdf",
    "pdfjs"
  ],
  "license": "MIT",
  "repository": {
    "url": "https://github.com/leo-buneev/vue-pdf-cdn/",
    "type": "git"
  }
}
