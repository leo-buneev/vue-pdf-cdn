{
  "name": "vue-pdf-cdn",
  "version": "1.0.10",
  "private": false,
  "description": "Lightweight PDF viewer for Vue.js using CDN",
  "author": "leo-buneev <buneev.leonid@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js --open",
    "build": "vue-cli-service build --name vue-pdf-cdn --entry ./src/index.js --target lib",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "vue-cli-service test:unit"
  },
  "main": "dist/vue-pdf-cdn.umd.js",
  "module": "dist/vue-pdf-cdn.common.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.5.17",
    "vue-resize-sensor": "^2.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.8",
    "@vue/cli-plugin-unit-jest": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "@vuedoc/md": "^1.3.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "vue-cli-plugin-component": "^1.7.1",
    "vue-template-compiler": "^2.5.17"
  },
  "keywords": [
    "vue",
    "component",
    "pdf",
    "pdfjs"
  ],
  "license": "MIT",
  "repository": {
    "url": "https://github.com/leo-buneev/vue-pdf-cdn/",
    "type": "git"
  }
}
